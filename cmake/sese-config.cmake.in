@PACKAGE_INIT@

message(STATUS "SESE_DB_USE_SQLITE: ${SESE_DB_USE_SQLITE}")
message(STATUS "SESE_DB_USE_MARIADB: ${SESE_DB_USE_MARIADB}")
message(STATUS "SESE_DB_USE_POSTGRES: ${SESE_DB_USE_POSTGRES}")
message(STATUS "SESE_USE_ARCHIVE: ${SESE_USE_ARCHIVE}")

include(CMakeFindDependencyMacro)

if (NOT BUILD_SHARED_LIBS)
    find_dependency(OpenSSL)
    find_dependency(ZLIB)
    find_dependency(asio)

    if (SESE_DB_USE_SQLITE)
        find_dependency(unofficial-sqlite3)
    endif ()
    if (SESE_DB_USE_MARIADB)
        find_dependency(unofficial-libmariadb)
    endif ()
    if (SESE_DB_USE_POSTGRES)
        find_dependency(PostgreSQL)
    endif ()
    if (SESE_USE_ARCHIVE)
        find_dependency(LibArchive)
    endif ()

endif ()

include("${CMAKE_CURRENT_LIST_DIR}/SeseTargets.cmake")
check_required_components("@PROJECT_NAME@")